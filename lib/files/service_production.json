{
    "id": "rw_adapter_cartodb_v1",
    "name": "RW Adapter CartoDB v1",
    "tags": ["rw"],
    "urls": [
      {
        "url": "/query/:dataset",
        "method": "GET",
        "paramProvider": "dataset",
        "dataProvider": "dataset",
        "filters":{
            "provider": "cartodb"
        },
        "endpoints": [{
            "method": "POST",
            "baseUrl": "http://localhost:3005",
            "path": "/query/:dataset",
            "data": ["dataset"]
        }]
      },
      {
        "url": "/fields/:dataset",
        "method": "GET",
        "paramProvider": "dataset",
        "dataProvider": "dataset",
        "filters":{
            "provider": "cartodb"
        },
        "endpoints": [{
            "method": "POST",
            "baseUrl": "http://localhost:3005",
            "path": "/fields/:dataset",
            "data": ["dataset"]
        }]
      },
      {
        "url": "/rest-datasets/cartodb",
        "method": "POST",
        "authenticated": true,
        "endpoints": [{
            "method": "POST",
            "baseUrl": "http://localhost:3005",
            "path": "/datasets"
        }]
      },
      {
        "url": "/rest-datasets/cartodb/:id",
        "method": "POST",
        "authenticated": true,
        "endpoints": [{
            "method": "POST",
            "baseUrl": "http://localhost:3005",
            "path": "/datasets/:id"
        }]
      },
      {
        "url": "/rest-datasets/cartodb/:id",
        "method": "DELETE",
        "authenticated": true,
        "endpoints": [{
            "method": "DELETE",
            "baseUrl": "http://localhost:3005",
            "path": "/datasets/:id"
        }]
      }
    ],
    "swagger": {}
}
